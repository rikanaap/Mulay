<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="330dp"
    android:layout_height="wrap_content"
    android:paddingBottom="20dp"
    android:background="@drawable/background_expandablelist">

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imageSetProfilePhotoLogout"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="15dp"
        android:src="@drawable/image_profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textSetDialogLogout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/outfit_regular"
        android:gravity="center_horizontal"
        android:includeFontPadding="false"
        android:textColor="@color/white"
        android:textSize="15sp"
        app:layout_constraintTop_toBottomOf="@id/imageSetProfilePhotoLogout"
        app:layout_constraintEnd_toEndOf="@id/imageSetProfilePhotoLogout"
        app:layout_constraintStart_toStartOf="@id/imageSetProfilePhotoLogout"/>

    <ImageView
        android:id="@+id/imageConfirmLogOut"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:src="@drawable/ic_logout_out"
        android:scaleType="fitXY"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@id/textSetDialogLogout"
        app:layout_constraintEnd_toEndOf="@id/textSetDialogLogout" />

    <ImageView
        android:id="@+id/imageCancelLogout"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:scaleType="fitXY"
        android:src="@drawable/ic_logout_cancel"
        app:layout_constraintTop_toTopOf="@id/imageConfirmLogOut"
        app:layout_constraintStart_toStartOf="@id/textSetDialogLogout" />
</androidx.constraintlayout.widget.ConstraintLayout>